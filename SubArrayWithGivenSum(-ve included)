//Negative Number

int subArraySum(int arr[], int n, int sum)
{
   unorderd_map<int,int> mp;
   int curr_sum = 0;
   for(int i = 0; i<n; i++)
   {
    curr_sum + arr[i];
    if(mp.find(curr_sum-sum) != mp.find())
    {
      cout<<"found"<<map[curr_sum-sum]+1<<i;
      return 1;
    }
    mp[curr_sum] = i;
   }
}
